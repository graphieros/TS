{
  "name": "turbo-spark",
  "private": false,
  "version": "0.0.2",
  "type": "module",
  "description": "A super light and highly customizable TS chart library that can be included in any framework.",
  "keywords": [
    "chart",
    "line",
    "bar",
    "stacked",
    "agnostic",
    "data storytelling",
    "data",
    "Vue",
    "React",
    "Angular",
    "Vanilla",
    "Typescript"
  ],
  "auhor": "Alec Lloyd Probert",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphieros/TS"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/turbo-spark.cjs",
  "module": "./dist/turbo-spark.js",
  "exports": {
    ".": {
      "import": "./dist/turbo-spark.js",
      "require": "./dist/turbo-spark.cjs",
      "types": "./dist/types/turbo-spark.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode production && node copy-types.cjs",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
